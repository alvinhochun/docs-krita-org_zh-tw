#: ../../reference_manual/main_menu/edit_menu.rst:1
msgid "The edit menu in Krita."
msgstr "Krita 的「編輯」選單。"

#: ../../reference_manual/main_menu/edit_menu.rst:16
msgid "Edit Menu"
msgstr "「編輯」選單"

#: ../../reference_manual/main_menu/edit_menu.rst:19
#: ../../reference_manual/main_menu/edit_menu.rst:11
msgid "Undo"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:21
msgid "Undoes the last action. Shortcut: :kbd:`Ctrl + Z`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:22
#: ../../reference_manual/main_menu/edit_menu.rst:11
msgid "Redo"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:24
msgid "Redoes the last undone action. Shortcut: :kbd:`Ctrl + Shift+ Z`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:25
#: ../../reference_manual/main_menu/edit_menu.rst:11
msgid "Cut"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:27
msgid "Cuts the selection or layer. Shortcut: :kbd:`Ctrl + X`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:28
#: ../../reference_manual/main_menu/edit_menu.rst:11
msgid "Copy"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:30
msgid "Copies the selection or layer. Shortcut: :kbd:`Ctrl + C`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:31
msgid "Cut (Sharp)"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:33
msgid ""
"This prevents semi-transparent areas from appearing on your cut pixels, "
"making them either fully opaque or fully transparent."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:34
msgid "Copy (Sharp)"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:36
msgid "Same as :term:`Cut (Sharp)` but then copying instead."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:37
msgid "Copy Merged"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:39
msgid "Copies the selection over all layers. Shortcut: :kbd:`Ctrl + Shift + C`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:40
#: ../../reference_manual/main_menu/edit_menu.rst:11
msgid "Paste"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:42
msgid ""
"Pastes the copied buffer into the image as a new layer. Shortcut: :kbd:`Ctrl "
"+ V`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:43
msgid "Paste at Cursor"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:45
msgid ""
"Same as :term:`Paste`, but aligns the image to the cursor. Shortcut: :kbd:"
"`Ctrl + Alt + V`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:46
msgid "Paste into Active Layer"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:48
msgid "Pastes the copied buffer into the current layer as a new selection."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:51
msgid "Paste into New Image"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:53
msgid ""
"Pastes the copied buffer into a new image. Shortcut: :kbd:`Ctrl + Shift + N`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:54
msgid "Paste as Reference Image"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:56
msgid ""
"Pastes the selection as a new :ref:`Reference Image <reference_images_tool>`."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:57
msgid "Paste Shape Style"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:59
msgid ""
"Used with :ref:`shape_selection_tool`, this allows you to copy the style "
"(the fill, outline and markers) of one vector shape to another."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:62
msgid "Clear"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:64
msgid "Empty the currently selected area or layer. Shortcut: :kbd:`Del`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:65
msgid "Fill with Foreground Color"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:67
msgid ""
"Fills the layer or selection with the foreground color without taking into "
"account blending modes or opacity. Shortcut: :kbd:`Shift + Backspace`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:68
msgid "Fill with Background Color"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:70
msgid ""
"Fills the layer or selection with the background color without taking into "
"account blending modes or opacity. Shortcut: :kbd:`Backspace`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:71
msgid "Fill with Pattern"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:73
msgid ""
"Fills the layer or selection with the active pattern without taking into "
"account blending modes or opacity."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:74
msgid "Fill with Foreground Color (Opacity)"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:76
msgid ""
"Fills the layer or selection with the foreground color, taking blending "
"modes and opacity into account. Shortcut: :kbd:`Ctrl + Shift + Backspace`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:77
msgid "Fill with Background Color (Opacity)"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:79
msgid ""
"Fills the layer or selection with the background color, taking blending "
"modes and opacity into account. Shortcut: :kbd:`Ctrl + Backspace`"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:80
msgid "Fill with Pattern (Opacity)"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:82
msgid ""
"Fills the layer or selection with the active pattern, taking blending modes "
"and opacity into account."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:83
msgid "Stroke Selected Shapes"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:85
msgid ""
"Strokes the selected vector shape with the selected brush, will create a new "
"layer."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:86
msgid "Stroke Selection"
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:88
msgid "Strokes the active selection using the menu."
msgstr ""

#: ../../reference_manual/main_menu/edit_menu.rst:11
msgid "Edit"
msgstr ""
