msgstr ""

#: ../../reference_manual/import_animation.rst:153
msgid "FFMpeg Path"
msgstr ""

#: ../../reference_manual/import_animation.rst:154
msgid "Path to the ffmpeg executable, necessary for importing the video data."
msgstr ""

#: ../../reference_manual/import_animation.rst:156
msgid "FFProbe path"
msgstr ""

#: ../../reference_manual/import_animation.rst:156
msgid ""
"Path to the ffmpeg probe executable, which is included with ffmpeg. This "
"allows Krita to learn the video information from the file."
msgstr ""

