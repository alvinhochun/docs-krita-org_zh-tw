#: ../../reference_manual/blending_modes/mix.rst:1
msgid "Page about the mix blending modes in Krita: Allanon, Alpha Darken, Behind, Erase, Geometric Mean, Grain Extract, Grain Merge, Greater, Hard Mix, Hard Overlay, Interpolation, Interpolation2x, Normal, Overlay, Parallel, Penumbra A, B, C and D."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:17
msgid "Mix"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:23
#: ../../reference_manual/blending_modes/mix.rst:19
msgid "Allanon"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:27
msgid "This blending mode is called \"Allanon\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:29
msgid "Blends the upper layer as half-transparent with the lower. (It add the two layers together and then halves the value)."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:34
msgid "Left: **Normal**. Right: **Allanon**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:40
#: ../../reference_manual/blending_modes/mix.rst:36
msgid "Interpolation"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:44
msgid "This blending mode is called \"Interpolation\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:46
msgid "Subtract 0.5f by 1/4 of cosine of base layer subtracted by 1/4 of cosine of blend layer assuming 0-1 range. The result is similar to Allanon mode, but with more contrast and functional difference to 50% opacity."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:52
msgid "Left: **Normal**. Right: **Interpolation**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:58
msgid "Interpolation - 2X"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:62
msgid "This blending mode is called \"Interpolation - 2X\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:64
msgid "Applies Interpolation blend mode to base and blend layers, then duplicate to repeat interpolation blending."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:69
msgid "Left: **Normal**. Right: **Interpolation - 2X**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:75
#: ../../reference_manual/blending_modes/mix.rst:71
msgid "Alpha Darken"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:79
msgid "This blending mode is called \"Alpha Darken\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:81
msgid "As far as I can tell this seems to premultiply the alpha, as is common in some file-formats."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:86
msgid "Left: **Normal**. Right: **Alpha Darken**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:92
#: ../../reference_manual/blending_modes/mix.rst:88
msgid "Behind"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:96
msgid "This blending mode is called \"Behind\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:98
msgid "Does the opposite of normal, and tries to have the upper layer rendered below the lower layer."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:103
msgid "Left: **Normal**. Right: **Behind**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:109
msgid "Erase"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:113
msgid "This blending mode is called \"Erase\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:115
msgid "This subtracts the opaque pixels of the upper layer from the lower layer, effectively erasing."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:120
msgid "Left: **Normal**. Right: **Erase**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:126
#: ../../reference_manual/blending_modes/mix.rst:122
msgid "Geometric Mean"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:130
msgid "This blending mode is called \"Geometric Mean\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:132
msgid "This blending mode multiplies the top layer with the bottom, and then outputs the square root of that."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:137
msgid "Left: **Normal**. Right: **Geometric Mean**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:143
#: ../../reference_manual/blending_modes/mix.rst:139
msgid "Grain Extract"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:147
msgid "This blending mode is called \"Grain Extract\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:149
msgid "Similar to subtract, the colors of the upper layer are subtracted from the colors of the lower layer, and then 50% gray is added."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:154
msgid "Left: **Normal**. Right: **Grain Extract**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:160
#: ../../reference_manual/blending_modes/mix.rst:156
msgid "Grain Merge"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:164
msgid "This blending mode is called \"Grain Merge\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:166
msgid "Similar to addition, the colors of the upper layer are added to the colors, and then 50% gray is subtracted."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:171
msgid "Left: **Normal**. Right: **Grain Merge**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:177
msgid "Greater"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:181
msgid "This blending mode is called \"Greater\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:183
msgid "A blending mode which checks whether the painted color is painted with a higher opacity than the existing colors. If so, it paints over them, if not, it doesn't paint at all."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:192
#: ../../reference_manual/blending_modes/mix.rst:188
msgid "Hard Mix"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:196
msgid "This blending mode is called \"Hard Mix\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:198
msgid "Similar to Overlay."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:200
msgid "Mixes both Color Dodge and Burn blending modes. If the color of the upper layer is darker than 50%, the blending mode will be Burn, if not the blending mode will be Color Dodge."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:206
msgid "Left: **Normal**. Right: **Hard Mix**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:212
#: ../../reference_manual/blending_modes/mix.rst:208
msgid "Hard Mix (Photoshop)"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:216
msgid "This blending mode is called \"Hard Mix (Photoshop)\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:218
msgid "This is the hard mix blending mode as it is implemented in photoshop."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:224
msgid "**Left**: Dots are mixed in with the normal blending mode, on the **Right**: Dots are mixed in with hardmix."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:226
msgid "This add the two values, and then checks if the value is above the maximum. If so it will output the maximum, otherwise the minimum."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:232
#: ../../reference_manual/blending_modes/mix.rst:228
msgid "Hard Mix Softer (Photoshop)"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:236
msgid "This blending mode is called \"Hard Mix Softer (Photoshop)\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:240
msgid "This is the hard mix blending mode as it is implemented in photoshop for texturing brushes. It produces softer edges than the normal *Hard Mix (Photoshop)*."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:247
msgid "**Left**: Dots are mixed in with the normal blending mode, on the **Right**: Dots are mixed in with hard mix softer."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:249
msgid "This is like the inverse subtract mode but the two terms are scaled up to increase the contrast. This is not really a hard mix mode in the sense that it doesn't choose between a result or another based on a threshold, although in most cases the result looks like the normal *Hard Mix (Photoshop)* but with softer edges."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:257
#: ../../reference_manual/blending_modes/mix.rst:253
msgid "Hard Overlay"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:261
msgid "This blending mode is called \"Hard Overlay\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:265
msgid "Similar to Hard light but hard light use Screen when the value is above 50%. Divide gives better results than Screen, especially on floating point images."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:270
msgid "Left: **Normal**. Right: **Hard Overlay**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:276
msgid "Normal"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:280
msgid "This blending mode is called \"Normal\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:282
msgid "As you may have guessed this is the default Blending mode for all layers."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:284
msgid "In this mode, the computer checks on the upper layer how transparent a pixel is, which color it is, and then mixes the color of the upper layer with the lower layer proportional to the transparency."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:289
msgid "Left: **Normal** 100% Opacity. Right: **Normal** 50% Opacity."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:295
msgid "Overlay"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:299
msgid "This blending mode is called \"Overlay\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:301
msgid "A combination of the Multiply and Screen blending modes, switching between both at a middle-lightness."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:303
msgid "Overlay checks if the color on the upperlayer has a lightness above 0.5. If so, the pixel is blended like in Screen mode, if not the pixel is blended like in Multiply mode."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:305
msgid "This is useful for deepening shadows and highlights."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:310
msgid "Left: **Normal**. Right: **Overlay**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:316
#: ../../reference_manual/blending_modes/mix.rst:312
msgid "Parallel"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:320
msgid "This blending mode is called \"Parallel\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:322
msgid "This one first takes the percentage in two decimal behind the comma for both layers. It then adds the two values. Divides 2 by the sum."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:329
msgid "Left: **Normal**. Right: **Parallel**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:335
#: ../../reference_manual/blending_modes/mix.rst:331
msgid "Penumbra A"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:339
msgid "This blending mode is called \"Penumbra A\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:341
msgid "Creates a linear penumbra falloff. This means most tones will be in the midtone ranges."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:346
msgid "Left: **Normal**. Right: **Penumbra A**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:352
#: ../../reference_manual/blending_modes/mix.rst:348
msgid "Penumbra B"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:356
msgid "This blending mode is called \"Penumbra B\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:358
msgid "Penumbra A with source and destination layer swapped."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:363
msgid "Left: **Normal**. Right: **Penumbra B**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:369
#: ../../reference_manual/blending_modes/mix.rst:365
msgid "Penumbra C"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:373
msgid "This blending mode is called \"Penumbra C\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:375
msgid "Creates a penumbra-like falloff using arc-tangent formula. This means most tones will be in the midtone ranges."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:380
msgid "Left: **Normal**. Right: **Penumbra C**."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:386
#: ../../reference_manual/blending_modes/mix.rst:382
msgid "Penumbra D"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:390
msgid "This blending mode is called \"Penumbra D\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:392
msgid "Penumbra C with source and destination layer swapped."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:397
msgid "Left: **Normal**. Right: **Penumbra D**."
